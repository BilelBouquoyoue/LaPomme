        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongoose": "^5.12.7",
        "multer": "^1.4.2",
        "path": "^0.12.7",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
